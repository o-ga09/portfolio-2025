		"@testing-library/react": "^16.3.0",
		"@types/jest-axe": "^3.5.9",
		"@types/node": "^20.17.50",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@vitejs/plugin-react": "^4.5.0",
		"@vitest/ui": "^3.1.4",
		"axe-core": "^4.10.3",
		"eslint": "^9",
		"eslint-config-next": "15.3.2",
